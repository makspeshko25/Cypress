{
  "name": "cypress",
  "version": "1.0.0",
  "description": "This repository is for learning Cypress",
  "main": "index.js",
  "scripts": {
    "test": "npx cypress run",
    "test:open": "npx cypress open",
    "test:headless": "npx cypress run --headless",
    "test:chrome": "npx cypress run --browser chrome",
    "test:firefox": "npx cypress run --browser firefox",
    "cypress:dev": "npx cypress open --env environment=dev",
    "cypress:dev:run": "npx cypress run --env environment=dev",
    "cypress:prod": "npx cypress open --env environment=prod",
    "cypress:prod:run": "npx cypress run --env environment=prod"
  },
